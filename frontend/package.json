{
  "name": "storyforge-ai-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:safe": "node scripts/test-safe.js",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:safe": "node scripts/test-safe.js --coverage",
    "test:cleanup": "node scripts/test-cleanup.js",
    "test:single": "node scripts/test-safe.js",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:uat": "ts-node --esm tests/e2e/run-uat.ts",
    "test:uat:ci": "ts-node --esm tests/e2e/run-uat.ts --ci",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "type-check": "tsc --noEmit",
    "pretest": "node scripts/test-cleanup.js",
    "posttest": "node scripts/test-cleanup.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@mui/x-data-grid": "^6.10.1",
    "@react-three/drei": "^9.82.0",
    "@react-three/fiber": "^8.13.6",
    "@reduxjs/toolkit": "^1.9.5",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "axios": "^1.11.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.1",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "lodash-es": "^4.17.21",
    "playwright": "^1.55.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^13.2.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9",
    "socket.io-client": "^4.7.2",
    "three": "^0.155.0",
    "use-debounce": "^9.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^24.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^7.0.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^1.6.0"
  }
}
