{
  "name": "storyforge-ai-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "build:tokens": "node scripts/build-tokens.mjs",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "lint:tsx:hex": "node scripts/lint-hex-tsx.mjs",
    "lint:all": "npm run lint && npm run lint:styles && npm run lint:tsx:hex",
    "lint:styles": "stylelint \"src/**/*.{css,scss}\"",
    "tokens:check": "node scripts/check-tokens-contrast.mjs",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "test": "vitest",
    "test:safe": "node scripts/test-safe.js",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:safe": "node scripts/test-safe.js --coverage",
    "test:cleanup": "node scripts/test-cleanup.js",
    "test:single": "node scripts/test-safe.js",
    "test:e2e": "playwright test && node scripts/generate-experience-report.mjs",
    "test:e2e:smoke": "playwright test --grep \"@experience-offline|@landing-smoke\" && node scripts/generate-experience-report.mjs",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:uat": "ts-node --esm tests/e2e/run-uat.ts",
    "test:uat:ci": "ts-node --esm tests/e2e/run-uat.ts --ci",
    "playwright:install": "playwright install",
    "playwright:install-deps": "playwright install-deps",
    "type-check": "tsc -p tsconfig.typecheck.json --noEmit",
    "size": "size-limit",
    "pretest": "node scripts/test-cleanup.js",
    "posttest": "node scripts/test-cleanup.js"
  },
  "size-limit": [
    {
      "name": "Initial Bundle",
      "path": "dist/assets/index-*.js",
      "limit": "400 KB",
      "gzip": true
    },
    {
      "name": "Route Chunks",
      "path": "dist/assets/*-*.js",
      "limit": "200 KB",
      "gzip": true,
      "ignore": [
        "dist/assets/index-*.js",
        "dist/assets/vendor-*.js"
      ]
    }
  ],
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.5",
    "@mui/x-data-grid": "^6.10.1",
    "@react-three/drei": "^9.82.0",
    "@react-three/fiber": "^8.13.6",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.11.0",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.1",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash-es": "^4.17.22",
    "markdown-it": "^14.1.0",
    "playwright": "^1.55.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^13.2.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9",
    "socket.io-client": "^4.8.3",
    "three": "^0.182.0",
    "use-debounce": "^9.0.4",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@axe-core/react": "^4.11.0",
    "@eslint/js": "^9.39.2",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.54.1",
    "@size-limit/file": "^11.1.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.3.0",
    "jest-axe": "^9.0.0",
    "jsdom": "^24.1.0",
    "rollup": "^3.29.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "size-limit": "^11.1.0",
    "stylelint": "^16.10.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard": "^36.0.1",
    "terser": "^5.44.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.5.5",
    "vitest": "^4.0.16"
  }
}
